class Solution {
  public:
    
    vector<long long> factorialNumbers(long long n) {
        // Write Your Code here
       vector<long long> vect;
       long long x;
       x=1;
       vect.push_back(x);
       for(int i=2;x<n;i++)
       {
            x=x*i;
            if(x<=n)
                vect.push_back(x);
            
       }
       return vect;
        
    }
};
